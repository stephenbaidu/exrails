<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <style type="text/css">
      .report body {
        padding: 0px;
      }
      .report .page-break {
        page-break-after: always;
      }
      .report .left {
        text-align: left;
      }
      .report .right {
        text-align: right;
      }
      .report .center {
        text-align: center;
      }
      .report .report-title {
        text-align: center;
        font-size: 20px;
        background-color: #ecebeb;
        margin: 0px 0px 20px 0px;
        padding: 5px;
        font-weight: bold;
      }
      .report .logo {
        float: right;
      }
      .report .footer {
        font-size: 9px;
        text-align: center;
        border-top: dotted 1px #EC8888;
        padding-top: 2px;
      }
      .report .report-table {
        font-size: 12px;
      }
      .report .report-table th, .report .report-table td {
        border: 1px solid black !important;
        padding: 3px;
      }
      .report .report-table th {
        font-size: 20px;
      }
    </style>
  </head>
  <div class="report">
    <%= yield %>
  </div>
</html>
